                                </dx:DXTabItem>-->


                                <dx:DXTabControl.BackgroundTemplate>
                                    <DataTemplate>
                                        <dxi:TabBorder xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                                                   BorderBackground="#FFF8F8F8" ViewInfo="{Binding}"/>
                                    </DataTemplate>
                                </dx:DXTabControl.BackgroundTemplate>

                                <dx:DXTabControl.ItemContainerStyle>
                                    <Style TargetType="dx:DXTabItem">
                                        <Setter Property="AllowHide" Value="True"/>
                                        <Setter Property="CloseCommand" Value="{Binding RelativeSource={RelativeSource AncestorType=dx:DXTabControl}, Path=DataContext.IItemCloseCommand}"/>
                                        <Setter Property="CloseCommandParameter" Value="{Binding ElementName=dxTabControl}"/>

                                        <!--색상 제어-->
                                        <Setter Property="HoverBackgroundTemplate" Value="{StaticResource TabItemHoverBackground}"/>
                                        <Setter Property="NormalBackgroundTemplate" Value="{StaticResource TabItemNormalBackground}"/>
                                        <Setter Property="SelectedBackgroundTemplate" Value="{StaticResource TabItemSelectedBackground}"/>
                                        <Setter Property="FocusedBackgroundTemplate" Value="{StaticResource TabItemFocusedBackground}"/>
                                    </Style>
                                </dx:DXTabControl.ItemContainerStyle>

                                <dxmvvm:Interaction.Behaviors>
                                    <dxmvvm:EventToCommand  EventName="SelectionChanged" CommandParameter="{Binding ElementName=dxTabControl}"  Command="{Binding ISelectedDxTabControl}"/>

                                </dxmvvm:Interaction.Behaviors>





                                <dx:DXTabControl.View>
                                    <dx:TabControlStretchView   x:Name="dxstretchview"
                                                                RemoveTabItemsOnHiding="True" 
                                                                HideButtonShowMode="InActiveTab"
                                                                MoveItemsWhenDragDrop="False">
                                    </dx:TabControlStretchView>

                                </dx:DXTabControl.View>
                            </dx:DXTabControl>

                        </Grid>

                    </dxdo:LayoutPanel>


                </dxdo:LayoutGroup>
            </dxdo:DockLayoutManager>

        </DockPanel>
